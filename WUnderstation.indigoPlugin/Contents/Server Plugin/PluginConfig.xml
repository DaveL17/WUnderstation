<?xml version="1.0"?>
<PluginConfig>
    <Field id="infoLabel00" type="label">
        <Label>WUnderstation Plugin</Label>
    </Field>

    <Field id="space000" type="label" fontSize="small">
        <Label>Developed by DaveL17</Label>
    </Field>

    <Field id="termsOfService" type="label" fontSize="mini">
        <Label>Personal Weather Station (PWS) service provided by Weather Underground, LLC. This plugin and its author are in no way affiliated with Weather Underground.</Label>
    </Field>

    <Field id="space001" type="label" alignText="right">
        <Label>Plugin Settings</Label>
    </Field>

	<Field id="simpleSeparator0" type="separator"/>

    <Field id="wunderstationID" type="textfield" defaultValue="" tooltip="Please enter your ID as registered by wunderground.com.">
        <Label>Station ID (PWS):</Label>
    </Field>

    <Field id="wunderstationPassword" type="textfield" defaultValue="" tooltip="Please enter your password as registered by wunderground.com.">
        <Label>Password:</Label>
    </Field>

    <Field id="uploadInterval" type="textfield" defaultValue="900" tooltip="Please select the desired frequency for data uploads.">
        <Label>Upload Frequency (seconds):</Label>
    </Field>

    <Field id="space06b" type="label"/>

    <Field id="space06a" type="label" alignText="Right">
        <Label>Data Settings</Label>
    </Field>

    <Field id="separator03" type="separator"/>

    <Field id="space006" type="checkbox">
        <Label>Display Weather Settings:</Label>
        <Description/>
    </Field>

    <!-- * for extra outdoor sensors use temp2f, temp3f, and so on -->
    <Field id="tempf1" type="menu" defaultValue="None" visibleBindingId="space006"  visibleBindingValue="true" tooltip="[F outdoor temperature]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Outdoor Temperature 1 (F)</Label>
    </Field>

    <Field id="tempf2" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[F outdoor temperature]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Outdoor Temperature 2 (F)</Label>
    </Field>

    <Field id="tempf3" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[F outdoor temperature]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Outdoor Temperature 3 (F)</Label>
    </Field>

    <Field id="tempf4" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[F outdoor temperature]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Outdoor Temperature 4 (F)</Label>
    </Field>

    <Field id="indoortempf" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[F indoor temperature F]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Indoor Temperature (F)</Label>
    </Field>

    <Field id="humidity" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[Percent outdoor humidity 0-100%]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Outdoor Humidity (0% - 100%)</Label>
    </Field>

    <Field id="indoorhumidity" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[Percent indoor humidity 0-100]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Indoor Humidity (0% - 100%)</Label>
    </Field>

    <Field id="baromin" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[barometric pressure inches]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Barometric Pressure (Inches)</Label>
    </Field>

    <Field id="dewptf" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[F outdoor dewpoint F]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Dewpoint (F)</Label>
    </Field>

    <Field id="rainin" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[rain inches over the past hour]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Rain Last Hour (Inches)</Label>
    </Field>

    <Field id="dailyrainin" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[rain inches so far today in local time]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Rain Today (Inches)</Label>
    </Field>

    <Field id="separator04" type="separator" visibleBindingId="space006" visibleBindingValue="true"/>

    <!-- * for sensors 2,3,4 use soiltemp2f, soiltemp3f, and soiltemp4f -->
    <Field id="soiltempf1" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[F soil temperature]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Soil Temperature 1 (F)</Label>
    </Field>

    <Field id="soiltempf2" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[F soil temperature]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Soil Temperature 2 (F)</Label>
    </Field>

    <Field id="soiltempf3" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[F soil temperature]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Soil Temperature 3 (F)</Label>
    </Field>

    <Field id="soiltempf4" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[F soil temperature]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Soil Temperature 4 (F)</Label>
    </Field>

    <!-- * for sensors 2,3,4 use soilmoisture2, soilmoisture3, and soilmoisture4 -->
    <Field id="soilmoisture1" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[Percent]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Soil Moisture 1 (0% - 100%)</Label>
    </Field>

    <Field id="soilmoisture2" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[Percent]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Soil Moisture 2 (0% - 100%)</Label>
    </Field>

    <Field id="soilmoisture3" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[Percent]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Soil Moisture 3 (0% - 100%)</Label>
    </Field>

    <Field id="soilmoisture4" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[Percent]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Soil Moisture 4 (0% - 100%)</Label>
    </Field>

    <!-- + for sensor 2 use leafwetness2 -->
    <Field id="leafwetness1" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[Percent]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Leaf Wetness 1 (0% - 100%)</Label>
    </Field>

    <Field id="leafwetness2" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[Percent]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Leaf Wetness 2 (0% - 100%)</Label>
    </Field>

    <Field id="separator05" type="separator" visibleBindingId="space006" visibleBindingValue="true"/>

    <Field id="weather" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[metar style (+RA)]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Weather (Text)</Label>
    </Field>

    <Field id="clouds" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[Text - SKC, FEW, SCT, BKN, OVC]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Clouds (Text)</Label>
    </Field>

    <Field id="solarradiation" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[W/m^2]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Solar Radiation</Label>
    </Field>

    <Field id="UV" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[Index]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>UV (Index)</Label>
    </Field>

    <Field id="visibility" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[nautical miles visibility]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Visibility (Nautical Miles)</Label>
    </Field>

    <Field id="separator07" type="separator" visibleBindingId="space006" visibleBindingValue="true"/>

    <Field id="windspeedmph" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[mph instantaneous wind speed]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Wind Speed (MPH)</Label>
    </Field>

    <Field id="winddir" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[0-360 instantaneous wind direction]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Wind Direction (0-360)</Label>
    </Field>

    <Field id="windgustmph" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[mph current wind gust, using software specific time period]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Wind Gust Speed (MPH)</Label>
    </Field>

    <Field id="windgustdir" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[0-360 using software specific time period]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Wind Gust Direction (0-360)</Label>
    </Field>

    <Field id="windspdmph_avg2m" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[mph 2 minute average wind speed mph]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Wind Speed (MPH/Avg. 2 min.)</Label>
    </Field>

    <Field id="winddir_avg2m" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[0-360 2 minute average wind direction]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Wind Direction (0-360/Avg. 2 min.)</Label>
    </Field>

    <Field id="windgustmph_10m" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[mph past 10 minutes wind gust mph]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Wind Gust (MPH/Avg. 10 min.)</Label>
    </Field>

    <Field id="windgustdir_10m" type="menu" defaultValue="None" visibleBindingId="space006" visibleBindingValue="true" tooltip="[0-360 past 10 minutes wind gust direction]">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Wind Direction (0-360/Avg. 10 min.)</Label>
    </Field>

    <Field id="separator08" type="separator" visibleBindingId="space006" visibleBindingValue="true"/>

    <!-- Air quality measures, hidden by default. -->
    <Field id="space09" type="checkbox">
        <Label>Display Air Quality Settings:</Label>
        <Description/>
    </Field>

    <Field id="AqUV-AETH" type="menu" defaultValue="None" visibleBindingId="space09" visibleBindingValue="true"
           tooltip="UV-AETH (second channel of Aethalometer at 370 nm for aromatic organic compounds) UG/M3">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Aethalometer (UG/M3)</Label>
    </Field>

    <Field id="AqBC" type="menu" defaultValue="None" visibleBindingId="space09" visibleBindingValue="true" tooltip="BC (black carbon at 880 nm) UG/M3">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Carbon - Black (UG/M3)</Label>
    </Field>

    <Field id="AqEC" type="menu" defaultValue="None" visibleBindingId="space09" visibleBindingValue="true" tooltip="EC (elemental carbon) – PM2.5 UG/M3">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Carbon - Elemental (UG/M3)</Label>
    </Field>

    <Field id="AqOC" type="menu" defaultValue="None" visibleBindingId="space09" visibleBindingValue="true"
           tooltip="OC (organic carbon, not adjusted for oxygen and hydrogen) – PM2.5 UG/M3">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Carbon - Organic (UG/M3)</Label>
    </Field>

    <Field id="AqCO" type="menu" defaultValue="None" visibleBindingId="space09" visibleBindingValue="true" tooltip="CO (carbon monoxide), conventional ppm">
        <List class="self" filter="" method="listOfVariables"/>
