    </Field>

    <Field id="AqNO2T" type="menu" defaultValue="None" visibleBindingId="space09" visibleBindingValue="true" tooltip="(nitrogen dioxide), true measure PPB">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Nitrogen Dioxide (PPB)</Label>
    </Field>

    <Field id="AqNO2" type="menu" defaultValue="None" visibleBindingId="space09" visibleBindingValue="true" tooltip="NO2 computed, NOx-NO PPB">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Nitrogen Dioxide (NOx-NO PPB)</Label>
    </Field>

    <Field id="AqNO2Y" type="menu" defaultValue="None" visibleBindingId="space09" visibleBindingValue="true" tooltip="NO2 computed, NOy-NO PPB">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Nitrogen Dioxide (NOy-NO PPB)</Label>
    </Field>

    <Field id="AqNOX" type="menu" defaultValue="None" visibleBindingId="space09" visibleBindingValue="true" tooltip="NOx (nitrogen oxides) - PPB">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Nitrogen Oxides (PPB)</Label>
    </Field>

    <Field id="AqNOY" type="menu" defaultValue="None" visibleBindingId="space09" visibleBindingValue="true" tooltip="NOy (total reactive nitrogen) - PPB">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Nitrogen - Total Reactive (PPB)</Label>
    </Field>

    <Field id="AqNO3" type="menu" defaultValue="None" visibleBindingId="space09" visibleBindingValue="true" tooltip="NO3 ion (nitrate, not adjusted for ammonium ion) UG/M3">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>NO3 Ion (UG/M3)</Label>
    </Field>

    <Field id="AqOZONE" type="menu" defaultValue="None" visibleBindingId="space09" visibleBindingValue="true" tooltip="Ozone - PPB">
        <List class="self" filter="" method="listOfVariables"/>
        <Label>Ozone (PPB)</Label>
    </Field>

    <Field id="AqPM2.5" type="menu" defaultValue="None" visibleBindingId="space09" visibleBindingValue="true" tooltip="PM2.5 mass - UG/M3">
